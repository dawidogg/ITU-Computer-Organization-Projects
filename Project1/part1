    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x557170686a60_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x557170686a60_0, 0, 2;
    %delay 170, 0;
    %vpi_call 2 54 "$display", "\012Decrement test" {0 0 0};
