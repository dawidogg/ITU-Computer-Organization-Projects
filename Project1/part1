S_0x55717066b310 .scope module, "register_tb" "register_tb" 2 3;
 .timescale 0 0;
v0x5571706868d0_0 .var "clk", 0 0;
v0x557170686990_0 .var "enable", 0 0;
v0x557170686a60_0 .var "funsel", 1 0;
